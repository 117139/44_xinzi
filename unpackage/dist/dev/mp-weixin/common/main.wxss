	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	position: relative;
	font-size: 18px;
	line-height: 40px;
	border: 1px solid #eee;
	margin-bottom: 30rpx;
}
.input-row .title {
	width: 40px;
	padding-left: 0;
	text-align: center;
	color: #999;
