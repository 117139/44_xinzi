
.wrap {
		width: 100%;
		background-color: #F5F5F5;
		background-image: url(data:image/png;base64,);
		background-size: 100% 170rpx;
		background-repeat: no-repeat;
}
.cx_list{
		width: 694rpx;
		background:rgba(255,255,255,1);
		box-shadow:0px 3px 18rpx 0px rgba(189,189,189,0.18);
		border-radius:10rpx;
		overflow: hidden;
		margin: 0 auto;
}
.cx_time {
		width: 100%;
		text-align: center;
		font-size: 16px;
		color: #fff;
		line-height: 55px;
}
.cx_time_pc{
		width: 694rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		padding: 0 10px;
		box-sizing: border-box;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		height: 40px;
		font-size: 16px;
}
picker.cx_time_pc{
		padding: 0;
		border-bottom: 1px solid #eee;
}
.sub_btn{
		width:694rpx;
		height:40px;
		background:rgba(41,119,223,1);
		border-radius:5px;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		font-size: 16px;
		color: #fff;
		margin: 50px auto;
}

